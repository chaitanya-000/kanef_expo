{
  "expo": {
    "plugins": ["expo-apple-authentication"],
    "name": "Kenaf",
    "slug": "kanef_with_expo",
    "version": "1.0.1",

    "icon": "./assets/images/kenafLogo.png",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "ie.kenaf.kenaf",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["*/"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ie.kenafltd.kenaf",
      "buildNumber": "12"
    },
    "android": {
      "package": "ie.kenaf.kenaf",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d1f029c0-c012-4d5d-80f6-2e004edaed87"
      }
    }
  }
}
